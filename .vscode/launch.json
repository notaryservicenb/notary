{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://127.0.0.1:5501",
      "webRoot": "${workspaceFolder}",
      "trace": true,
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*",
        "webpack:///src/*": "${webRoot}/src/*",
        "webpack:///./~/*": "${webRoot}/node_modules/*"
      },
      "sourceMaps": true
    }
  ]
}


